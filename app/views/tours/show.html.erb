<% title 'Tours' %>
<%= react_component('SingleTour', showUrl: tour_path + '.json',
								  points_url: points_path + '.json',
								  new_tour_point_url: create_tour_point_path,
								  tour_id: @tour.id,
								  pdfUrl: tour_pdf_path(@tour),
								  update_tour_url: update_tour_path(@tour),
								  audiences: @audiences,
									new_tour_session_url: create_tour_session_path,
									tourNote: @tour.notes,
									feedbackPostUrl: create_feedback_path ) %>
<div class="row">
	<h2>Feedback Statistics</h2>
	<%= pie_chart feedback_data_path(@tour['id'], width: '800px', height: '500px', xtitle: 'Ratings', ytitle: 'Ratings') %>
</div>
